# Base URL da API Ecuro Light (sem barra final)
ECURO_BASE_URL=https://clinics.api.ecuro.com.br/api/v1/ecuro-light

# Token de acesso (header app-access-token)
ECURO_APP_ACCESS_TOKEN=your-token-here

# Porta do servidor MCP (default: 3000)
PORT=3000

# Nível de log: debug | info | warn | error (default: info)
# debug = inclui requisições HTTP detalhadas e body MCP
LOG_LEVEL=info
